#include "aula0301.h"

ull
CalcularTermoSerieFibonacci (unsigned short numeroTermo)
{
    if (numeroTermo <= 1) return numeroTermo;

    return (CalcularTermoSerieFibonacci (numeroTermo - 1) + CalcularTermoSerieFibonacci (numeroTermo - 2));
}

