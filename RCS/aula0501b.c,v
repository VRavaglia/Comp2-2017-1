#include "aula0501.h"
#include "aula0401.h"

float
CalcularSerieHarmonicaAlternada (unsigned long int termos)
{
	float serieHarmonica = 0;
	unsigned long indice = termos;

	if (termos == 0) return 0;
	
	do
	{	
		if (indice % 2 == 0) 
		{	
			serieHarmonica -= CalcularExponencial(indice, -1 * indice);
		}
		else
		{
			serieHarmonica += CalcularExponencial(indice, -1 * indice);
		}

		indice--;
	}
	while(indice > 0);

	return serieHarmonica;
}
	

